    autoProgressLines: 00
    autoProgressDelay: 1.5
    typingSpeed: 0.05
    voiceSound: {fileID: 0}
  - stateName: PlotNotYetRestored
    englishLines:
    - "Patience, Little One\u2026 the forest can be healed if you search carefully."
    spanishLines:
    - "Paciencia, Peque\xF1a\u2026 el bosque se puede curar si buscas con cuidado."
    autoProgressLines: 00
    autoProgressDelay: 1.5
    typingSpeed: 0.05
    voiceSound: {fileID: 0}
